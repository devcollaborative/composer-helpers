    "type": "composer-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Jeff Hipp",
            "email": "jeff@devcollaborative.com"
        }
    ],
    "extra": {
        "class": "devcollaborative\\DrupalBranchStatus\\Plugin"
